{
  "name": "wayu-mx-landing-page",
  "version": "1.0.0",
  "description": "Wayu MX Landing version",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs",
    "laapp",
    "laapp-react",
    "design-react",
    "react-kit",
    "design-system-react"
  ],
  "author": "Wayu",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build docs",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/sass/laapp.scss src/assets/css/laapp.css",
    "minify-sass": "node-sass src/assets/sass/laapp.scss src/assets/css/laapp.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/sass/laapp.scss src/assets/css/laapp.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
